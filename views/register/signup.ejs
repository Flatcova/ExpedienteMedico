<%- layout('layout') %>
<div class="mdl-grid">
  <div class="mdl-cell mdl-cell--2-col mdl-cell--3-col-tablet mdl-cell--0-col-phone"></div>
  <div class="mdl-cell mdl-cell--8-col mdl-cell--12-col-tablet">
    <div class="mdl-card mdl-shadow--2dp through mdl-shadow--16dp card--register_card">
      <div class="mdl-card__title card--position_center">
          <h3>Registro</h3>
      </div>
      <div class="mdl-card__supporting-text">
        <!-- Formulario va aqui -->
      </div>
      <div class="mdl-card__actions mdl-card--border">
        <button type="button" class="button buttonBlue mdl-js-button mdl-js-ripple-effect">Registrarse
          <div class="ripples buttonRipples"><span class="ripplesCircle"></span></div>
        </button>
      </div>
    </div>
  </div>
  <div class="mdl-cell mdl-cell--2-col mdl-cell--3-col-tablet mdl-cell--0-col-phone"></div>
</div>
